<aura:component implements="forceCommunity:layout" access="global" description="PSA Dealer Detail Page Template">
    <aura:attribute name="left_1" type="Aura.Component[]" />
    <aura:attribute name="left_2" type="Aura.Component[]" />
    <aura:attribute name="right_1" type="Aura.Component[]" />

    <div class="slds-grid slds-wrap detail-page-container">
        <div class="slds-col slds-size_2-of-3 slds-medium-size_4-of-6 slds-large-size_9-of-12">
            <div class="left-1-container">
                {!v.left_1}
            </div>
            <div class="left-2-container">
                {!v.left_2}
            </div>
        </div>
        <div class="slds-col slds-size_1-of-3 slds-medium-size_2-of-6 slds-large-size_3-of-12">
            <div class="right-1-container">
                {!v.right_1}
            </div>
        </div>
    </div>

</aura:component>