<aura:component implements="forceCommunity:layout" access="global" description="PSA After Sales Home Page Template">
    <aura:attribute name="fullwidth_0" type="Aura.Component[]" />
    <aura:attribute name="fullwidth_1" type="Aura.Component[]" />
    <div class="slds-grid slds-wrap home-page-container">
        <div class="slds-col slds-size_3-of-3 slds-medium-size_6-of-6 slds-large-size_12-of-12">
            <div class="fullwidth-1-container">
                {!v.fullwidth_0}
            </div>
        </div>
        <div class="slds-col slds-size_3-of-3 slds-medium-size_6-of-6 slds-large-size_12-of-12">
            <div class="fullwidth-1-container">
                {!v.fullwidth_1 }
            </div>
        </div>
    </div>
</aura:component>