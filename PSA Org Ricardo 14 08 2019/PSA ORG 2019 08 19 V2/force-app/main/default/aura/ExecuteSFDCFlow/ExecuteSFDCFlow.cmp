<aura:component implements="flexipage:availableForAllPageTypes,force:hasRecordId,forceCommunity:availableForAllPageTypes" access="global">
	<aura:attribute name="recordId" type="String" default="" access="global" />
    <aura:attribute name="ActionName" type="String" default="" access="global" />
    <aura:attribute name="FlowName" type="String" default="" access="global" />
    
    <aura:handler name="init" value="{!this}" action="{!c.init}" />
    <lightning:flow aura:id="flowData" />
</aura:component>